./pingpong 16383 ${2} 9000 TCP ${1} >> TCP_${3}
./pingpong 32767 ${2} 9000 TCP ${1} >> TCP_${3}
./pingpong 65000 ${2} 9000 TCP ${1} >> TCP_${3}
