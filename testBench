#./pingpong 1 ${2} 9000 UDP ${1} >> UDP_${3}
#./pingpong 16383 ${2} 9000 UDP ${1} >> UDP_${3}
#./pingpong 32767 ${2} 9000 UDP ${1} >> UDP_${3}
#./pingpong 65000 ${2} 9000 UDP ${1} >> UDP_${3}

